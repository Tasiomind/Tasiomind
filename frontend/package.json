{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Tasiomind Frontend",
  "author": "Aland Mariwan",
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "build:apps": "turbo build --filter=./apps/*",
    "build:packages": "turbo build --filter=./packages/*",
    "check": "turbo lint && turbo type-check && pnpm format:check && pnpm check:spelling && pnpm check:knip",
    "check:knip": "VITE_CJS_IGNORE_WARNING=true dotenv -e .env.local -- knip",
    "check:npm": "pnpm dlx npm-check-updates --deep --interactive --format group",
    "check:spelling": "cspell --show-context --show-suggestions",
    "clean": "turbo clean && rm -rf .turbo coverage",
    "commit": "cz",
    "db:check": "turbo db:check",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "db:push": "turbo db:push",
    "db:seed": "turbo db:seed",
    "db:studio": "turbo db:studio",
    "dev": "turbo dev",
    "dev:packages": "turbo dev --filter=./packages/*",
    "format:check": "prettier --cache --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "format:write": "prettier --cache --write --list-different --ignore-path .gitignore --ignore-path .prettierignore .",
    "preinstall": "npx --yes only-allow pnpm",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix && pnpm format:write && tsx scripts/fix-cspell.ts",
    "prepare": "husky && pnpm build:packages",
    "release": "changeset publish",
    "test:e2e": "turbo test:e2e",
    "test:e2e:ui": "turbo test:e2e:ui",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest --coverage --ui",
    "test:unit:ui": "vitest --ui",
    "type-check": "turbo type-check",
    "version": "changeset version"
  },
  "dependencies": {
    "@apollo/client": "^3.10.8",
    "@fullcalendar/core": "^6.1.14",
    "@fullcalendar/daygrid": "^6.1.14",
    "@fullcalendar/interaction": "^6.1.14",
    "@fullcalendar/timegrid": "^6.1.14",
    "@fullcalendar/vue3": "^6.1.14",
    "@mdi/font": "7.4.47",
    "@mdi/js": "^7.4.47",
    "@vue/apollo-composable": "^4.0.2",
    "@vuepic/vue-datepicker": "^8.8.1",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^10.11.0",
    "@vueuse/integrations": "^10.11.0",
    "@yeger/vue-masonry-wall": "^5.0.12",
    "apexcharts": "3.49.2",
    "apollo-upload-client": "^18.0.1",
    "axios": "^1.7.2",
    "axios-mock-adapter": "^1.22.0",
    "chart.js": "^4.4.3",
    "cleave.js": "^1.6.0",
    "core-js": "^3.37.1",
    "crypto-js": "^4.2.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "gridstack": "^10.3.0",
    "js-base64": "^3.7.7",
    "keycloak-js": "^25.0.1",
    "libphonenumber-js": "^1.11.4",
    "luxon": "^3.4.4",
    "otpauth": "^9.3.1",
    "perfect-scrollbar": "^1.5.5",
    "pinia": "^2.1.7",
    "postcss-scss": "^4.0.9",
    "prismjs": "^1.29.0",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.77.6",
    "sortablejs": "^1.15.2",
    "swiper": "^11.1.4",
    "vee-validate": "^4.13.1",
    "vue": "^3.4.31",
    "vue-chartjs": "^5.3.1",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.13.1",
    "vue-prism-component": "^2.0.0",
    "vue-router": "^4.4.0",
    "vue-tel-input": "^9.1.4",
    "vue3-apexcharts": "^1.5.3",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vue3-promise-dialog": "^0.3.4",
    "vue3-toastify": "^0.2.2",
    "vuetify": "^3.6.11",
    "vuex": "^4.1.0",
    "webfontloader": "^1.6.28",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@eslint/js": "^9.6.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/devtools": "^7.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.7.0",
    "pinia": "^2.1.7",
    "postcss": "^8.4.39",
    "postcss-html": "^1.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "sass": "^1.77.6",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-use-logical-spec": "^5.0.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.2",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-devtools": "^7.3.5",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.6.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.24"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "resolutions": {
    "postcss": "8",
    "stylelint-order": "5",
    "postcss-sorting": "^7.0.1"
  }
}
